#!/bin/bash -e

# See comment in bootstrap_fun

# This script is used to run a bootstrap on Jenkins. For travis, see .travis.yml.

source scripts/common
source scripts/bootstrap_fun

determineScalaVersion
deriveModuleVersions

removeExistingBuilds $integrationRepoUrl

if [ ! -z "$STARR_REF" ]; then
  buildStarr
fi

buildLocker

buildQuick

invokeQuick testAll
testStability
